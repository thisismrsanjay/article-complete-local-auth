<% include partials/top.ejs %>


<form action="/users/register" method="POST">
    <div class="form-group mt-2">
        <label for="">Name</label>
        <input name='name' value="<%= (typeof name != "undefined" ? name : "")%>" type="text" class="form-control" required>
    </div>
    <div>
        <label for="">Password</label>
        <input name='password' value="<%= (typeof password != "undefined" ? password : "")%>" type="password" class="form-control" required>
    </div>
    <div>
        <label for="">Password ver</label>
        <input name='password2' value="<%= (typeof password2 != "undefined" ? password2 : "")%>" type="password" class="form-control" required>
    </div>
    <div class="form-group mt-2">
        <label for="">Email</label>
        <input name='email' value="<%= (typeof email != "undefined" ? email : "")%>" type="email" class="form-control" required>
    </div>
    <div class="form-group mt-2">
        <label for="">UserName</label>
        <input name='username' value="<%= (typeof username != "undefined" ? username : "")%>" type="text" class="form-control" required>
    </div>
    <input type="submit" value='Submit' class="btn mt-2">
</form>






<% include partials/bottom.ejs %>